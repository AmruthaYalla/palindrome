/*printing the fabinocii number at a particular index given by the user
*/
Problem Statement
import java.util.*;
import java.io.*;
public class Solution {
	static int fabi(int n){
		if(n==1)return 1;
		if(n==2)return 1;
	return fabi(n-1)+fabi(n-2);
	}
	public static void main(String [] args){
		Scanner sc=new Scanner(System.in);
		System.out.println(Solution.fabi(sc.nextInt()));
	}
}
